{
  "name": "ski-alpin-manager",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "dotenv -e .env.production node src/server.js",
    "dev": "dotenv -e .env.development nodemon src/server.js",
    "test": "dotenv -e .env.test jest --verbose",
    "test:ci": "dotenv -e .env.test jest --ci --coverage --detectOpenHandles",
    "test:watch": "dotenv -e .env.test jest --watchAll",
    "migrate": "node src/database/migrate.js",
    "migrate:dev": "dotenv -e .env.development node src/database/migrate.js",
    "migrate:test": "dotenv -e .env.test node src/database/migrate.js",
    "migrate:prod": "dotenv -e .env.production node src/database/migrate.js",
    "migrate:undo": "node src/database/migrate.js undo",
    "migrate:status": "node src/database/migrate.js status",
    "migrate:down": "dotenv -e .env.development node src/database/migrate.js down",
    "seed": "node src/database/seeds/seed.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write 'src/**/*.{js,json}'",
    "test:db": "dotenv -e .env.development node src/database/test-connection.js",
    "seed:dev": "dotenv -e .env.development node src/database/seeds/seed_teams.js",
    "seed:test": "dotenv -e .env.test node src/database/seeds/seed_teams.js",
    "db:reset": "npm run migrate:down && npm run migrate:dev && npm run seed:teams && npm run seed:athletes && npm run seed:stats",
    "seed:teams": "dotenv -e .env.development node src/database/seeds/seed_teams.js",
    "seed:athletes": "dotenv -e .env.development node src/database/seeds/seed_athletes.js",
    "db:check": "dotenv -e .env.development node src/database/check.js",
    "db:test": "dotenv -e .env.development node src/database/connection_test.js",
    "seed:stats": "dotenv -e .env.development node src/database/seeds/seed_athlete_stats.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.5",
    "connect-sqlite3": "^0.9.15",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-ejs-layouts": "^2.5.1",
    "express-flash": "^0.0.2",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.3",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.16",
    "pdfkit": "^0.15.1",
    "pg": "^8.13.1",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "dotenv-cli": "^7.4.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0"
  },
  "description": ""
}
